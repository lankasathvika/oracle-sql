---OBJECT TYPE
CREATE OR REPLACE TYPE student_obj AS OBJECT (
  id NUMBER,
  name VARCHAR2(50),
  FUNCTION get_upper_name RETURN VARCHAR2
);

---VARRAY
CREATE OR REPLACE TYPE grade_list AS VARRAY(5) OF NUMBER;

---NESTED TABLE
CREATE OR REPLACE TYPE course_list AS TABLE OF VARCHAR2(100);

---RECORD
DECLARE
  TYPE student_rec IS RECORD (
    id NUMBER,
    name VARCHAR2(50)
  );


